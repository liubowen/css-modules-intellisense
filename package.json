{
  "name": "css-modules-intellisense",
  "displayName": "css modules intellisense",
  "description": "css modules 智能提示",
  "version": "0.0.2",
  "keywords": [
    "css modules",
    "intellisense",
    "completion"
  ],
  "publisher": "pakwoon",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "css-modules-intellisense",
      "properties": {
        "css-modules-intellisense.alias": {
          "type": "object",
          "default": {},
          "description": "路径别名"
        },
        "css-modules-intellisense.configPath": {
          "type": "string",
          "default": "./cmconfig.json",
          "description": "设置当前项目配置文件地址"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/liubowen/css-modules-intellisense"
  },
  "bugs": {
    "url": "https://github.com/liubowen/css-modules-intellisense/issues"
  },
  "homepage": "https://github.com/liubowen/css-modules-intellisense/blob/master/README.md"
}
